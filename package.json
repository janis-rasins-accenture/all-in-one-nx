{
  "name": "@all-in-one-nx/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "lint:check": "cross-env NX_PLUGIN_NO_TIMEOUTS=true nx run-many -t lint --skip-nx-cache",
    "format:check": "prettier --ignore-path .gitignore --check \"**/*.{ts,tsx,json,js,jsx,html,yml,md,mdx,css}\" \"!**/dist/**\" \"!**/coverage/**\" \"!**/CHANGELOG.md\" \"!**/.github/workflows/**\"",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{ts,tsx,json,js,jsx,html,yml,md,mdx,css}\" \"!**/dist/**\" \"!**/coverage/**\" \"!**/CHANGELOG.md\" \"!**/.github/workflows/**\"",
    "test": "cross-env NX_PLUGIN_NO_TIMEOUTS=true nx run-many -t test --parallel=true --skip-nx-cache --coverage",
    "start:storybook": "cross-env NX_PLUGIN_NO_TIMEOUTS=true nx storybook storybook-host --skip-nx-cache",
    "start:express": "nx serve all-in-one-web-express-api --skip-nx-cache",
    "start": "cross-env NX_PLUGIN_NO_TIMEOUTS=true npx nx dev all-in-one-web --skip-nx-cache",
    "android": "cross-env NX_PLUGIN_NO_TIMEOUTS=true npx nx run-android all-in-one-native --skip-nx-cache",
    "ios": "cross-env NX_PLUGIN_NO_TIMEOUTS=true npx nx run-ios all-in-one-native --skip-nx-cache",
    "build": "nx run-many --target=build --all --exclude=storybook-host --skip-nx-cache",
    "build:buttons-native": "nx build --project=buttons-native --skip-nx-cache"
  },
  "private": true,
  "dependencies": {
    "@react-router/node": "^7.2.0",
